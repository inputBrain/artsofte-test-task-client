@model ArtsofteClient.API.Language.GetAllLanguageResponse


    <div class="row">
        <div class="col-md-3">
            @await Component.InvokeAsync("Sidebar")
        </div>
        <div class="col-md-9">
            <div class="container mt-5">
                <button type="button" class="btn btn-sm btn-info" disabled>Coming soon</button>
                <br>
                <table class="table table-hover" id="newsTable">
                    <thead>
                    <tr>
                        <th>Id</th>
                        <th>Language</th>
                        <th>Actions</th>
                    </tr>
                    </thead>
                    <tbody>
                    @foreach (var item in Model.Languages)
                    {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Language</td>
                        <td>
                            <div class="btn-group" role="group">
                                <button type="button" class="btn btn-sm btn-outline-warning" disabled>Coming soon</button>
                                <button type="button" class="btn btn-sm btn-outline-danger" disabled>Coming soon</button>
                            </div>
                        </td>
                    </tr>
                    }
                    </tbody>
                </table>
            </div>

        </div>
    </div>